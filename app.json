{
  "expo": {
    "name": "dado",
    "slug": "dado",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "dado",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "assetBundlePatterns": [
      "**/*",
      "assets/**/*",
      "assets/models/*.glb",
      "assets/models/*.gltf"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourname.dado"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.yourname.dado",
      "permissions": [
        "ACCELEROMETER"
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",

      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#bd93f9",
          "sounds": [
            "./assets/sounds/custom-sound.wav"
          ]
        }
      ],

      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-asset"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "4c14fc47-6488-4b49-9479-4946fe5b6b38"
      }
    }
  }
}